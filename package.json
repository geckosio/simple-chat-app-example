{
  "name": "chat-app-example",
  "version": "2.0.0",
  "description": "Simple chat app example with geckos.io",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:nodejs": "nodemon server.mjs",
    "dev:browser": "live-server --port=8080",
    "dev:esbuild": "esbuild js/index.js --outfile=js/bundle.js --bundle --watch",
    "build": "esbuild js/index.js --outfile=js/bundle.js --bundle"
  },
  "author": "Yannick Deubel (https://github.com/yandeu)",
  "license": "MIT",
  "dependencies": {
    "@geckos.io/client": "^2.3.0",
    "@geckos.io/server": "^2.3.0"
  },
  "devDependencies": {
    "esbuild": "^0.15.3",
    "five-server": "^0.1.4",
    "nodemon": "^2.0.12",
    "npm-run-all": "^4.1.5"
  }
}